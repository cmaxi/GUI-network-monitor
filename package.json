{
  "name": "Network-monitor",
  "version": "2.1.0",
  "description": "GUI for api app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "distW": "electron-builder --win",
    "distA": "electron-builder --mac",
    "distL": "electron-builder --linux snap"
  },
  "keywords": [
    "Electron",
    "GUI",
    "Network application"
  ],
  "files": [
    "index.html",
    "main.js",
    "preload.js",
    "settings.json",
    "pics/**/*",
    "certif/**/*"
  ],
  "repository": "https://github.com/Guestas/GUI-network-monitor",
  "publish": {
    "provider": "github",
    "releaseType": "release",
    "private": true,
    "prerelease": false
  },
  "build": {
    "appId": "com.MCNetworkMonitor.app",
    "productName": "Network Monitor app",
    "directories": {
      "output": "dist"
    },
    "win": {
      "icon": "ic/win.ico"
    },
    "mac": {
      "icon": "ic/mac.icns"
    },
    "linux": {
      "icon": "ic/linux.png"
    }
  },
